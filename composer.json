{
    "name": "dutchheight/craft-reviewer",
    "description": "Enables content editors to share a preview of a page, which can be viewed by a public or private URL.",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "page reviewer"
    ],
    "support": {
        "docs": "https://github.com/dutchheight/craft-reviewer/blob/master/README.md",
        "issues": "https://github.com/dutchheight/craft-reviewer/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Dutch Height",
            "homepage": "https://www.dutchheight.com/"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
          "dutchheight\\reviewer\\": "src/"
        }
    },
    "extra": {
        "name": "Page Reviewer",
        "handle": "reviewer",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/dutchheight/craft-reviewer/master/CHANGELOG.md",
        "components": {
            "comments": "dutchheight\\reviewer\\services\\Comments"
        },
        "class": "dutchheight\\reviewer\\Reviewer"
    }
}
